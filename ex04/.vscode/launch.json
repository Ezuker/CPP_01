{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug cub3d",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/Sed_is_for_losers",
        "args": ["Makefile", "echi", "test"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build"
      }
    ]
  }
